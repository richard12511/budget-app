<h1><%= @category.category_name %></h1>

<div class="row">
	<div class="col-md-12">
		<div class="col-md-12">User: <%= link_to @category.user.name, @category.user  %></div>
		<div class="col-md-12">Is System?: <%= @category.is_system %></div>
		<div class="col-md-12">Created at: <%= @category.created_at.strftime("%m/%d/%Y") %></div>
	</div>
</div>
